spu:
    [1 ~ ~ 2 ~ ~ 3 ~] 1000 0 track
    [0 127] linear 1000 1 track

mpu_out_ctrl:
    event_value     ; Pitch
    127             ; Velocity
    event_duration  ; Duration
    1               ; Channel
    makenote        ; Trigger an off event after duration has elapsed
    noteout         ; Dispatch the event

mpu_out_note:
    event_value     ; Pitch
    127             ; Velocity
    event_duration  ; Duration
    1               ; Channel
    noteout         ; Dispatch the event
